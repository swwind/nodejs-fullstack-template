{
  "name": "example",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEV=true node build/main.js",
    "clean": "rm -rf build dist",
    "build": "yarn clean && yarn build:client && yarn build:ssr && yarn build:server",
    "build:check": "vue-tsc --noEmit",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:ssr": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server": "webpack --mode=production",
    "start": "node build/main.js",
    "debug": "cross-env DEV=true node --inspect-brk build/main.js --debug",
    "lint": "yarn lint:client && yarn lint:server && yarn lint:tests",
    "lint:fix": "yarn lint:client --fix && yarn lint:server --fix && yarn lint:tests --fix",
    "lint:client": "eslint --ext ts,vue src -f pretty",
    "lint:server": "eslint --ext ts app -f pretty",
    "lint:tests": "eslint --ext ts tests -f pretty",
    "test": "cross-env TESTING=true node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/server-renderer": "^3.2.20",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.9",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-compress": "^5.1.0",
    "koa-connect": "^2.1.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "less": "^4.1.2",
    "mime-types": "^2.1.33",
    "minio": "^7.0.19",
    "mongodb": "^4.1.3",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.3",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa__cors": "^3.0.3",
    "@types/mime-types": "^2.1.1",
    "@types/minio": "^7.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "eslint": "^8.1.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "should": "^13.2.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "vite": "^2.6.12",
    "vue-tsc": "^0.29.6",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
